generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") // Will be picked from your .env
}

model UserProfile {
  id            String  @id @default(uuid())
  phoneNumber   String  @unique
  name          String?
  email         String?
  emailVerified Boolean @default(false)
}
